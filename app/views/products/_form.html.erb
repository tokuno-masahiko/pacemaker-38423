    <%= form_with(model: product, local: true) do |form| %>
      <%= form.text_field :image, placeholder: "Image Url" %>
      <%= form.text_area :maker, placeholder: "メーカー", rows: "10" %>
      <%= form.text_area :item, placeholder: "製品名", rows: "10" %>
      <%= form.number_field :battery, placeholder: "バッテリー年数", rows: "10" %>
      <%= form.submit "SEND" %>
    <% end %>